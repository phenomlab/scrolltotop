{
    "name": "phenomlab/scrolltotop",
    "description": "Enables a simple scroll to top button",
    "type": "flarum-extension",
    "keywords": ["UI", "scroll"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Cutting",
            "homepage": "https://phenomlab.com",
            "role": "Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/phenomlab/scrolltotop/issues",
        "source": "https://github.com/phenomlab/scrolltotop",
        "forum": "https://discuss.flarum.org"
    },
    "homepage": "https://paypal.me",
    "funding": [
        {
            "type": "website",
            "url": "https://phenomlab.com"
        }
    ],
    "require": {
        "flarum/core": ">=0.1.0-beta.16 <=0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Phenomlab\\Scrolltotop\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Scroll to Top",
            "category": "feature",
            "icon": {
                "name": "fas fa-chevron-up",
                "backgroundColor": "#666",
                "color": "#ffffff"
            }
        }
    }
}
